<?xml version="1.0"?>
<!DOCTYPE profile>
<profile xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
  <add-on>
    <add_on_products config:type="list">
      <listentry>
        <media_url>cd:///</media_url>
        <product>sle-module-basesystem-release</product>
        <product_dir>/Module-Basesystem</product_dir>
      </listentry>
      <listentry>
        <media_url>cd:///</media_url>
        <product>sle-module-server-applications</product>
        <product_dir>/Module-Server-Applications</product_dir>
      </listentry>
    </add_on_products>
  </add-on>
  <general>
    <mode>
      <confirm config:type="boolean">false</confirm>
      <second_stage config:type="boolean">false</second_stage>
    </mode>
    <proposals config:type="list"/>
  </general>
  <keyboard>
    <keymap>english-us</keymap>
  </keyboard>
  <language>
    <language>en_US</language>
    <languages/>
  </language>
  <login_settings/>
  <networking>
    <interfaces config:type="list">
      <interface>
        <bootproto>dhcp</bootproto>
        <device>eth0</device>
        <startmode>auto</startmode>
        <zone>internal</zone>
      </interface>
    </interfaces>
  </networking>
  <partitioning config:type="list">
    <drive>
      <use>all</use>
    </drive>
  </partitioning>
  <software>
    <install_recommended t="boolean">true</install_recommended>
    <patterns config:type="list">
      <pattern>base</pattern>
    </patterns>
    <products config:type="list">
      <product>SLES</product>
    </products>
    <packages config:type="list">
      <package>qemu-guest-agent</package>
    </packages>
  </software>
  <timezone>
    <hwclock>UTC</hwclock>
    <timezone>UTC</timezone>
  </timezone>
  <users config:type="list">
    <user>
      <encrypted config:type="boolean">false</encrypted>
      <username>root</username>
      <user_password>secret_root_password</user_password>
    </user>
    <user>
      <encrypted config:type="boolean">false</encrypted>
      <fullname>User</fullname>
      <gid>1000</gid>
      <home>/home/user</home>
      <shell>/bin/bash</shell>
      <uid>1000</uid>
      <user_password>root</user_password>
      <username>user</username>
    </user>
  </users>
  <groups config:type="list">
    <group>
      <groupname>wheel</groupname>
      <userlist>user</userlist>
    </group>
    <group>
      <groupname>user</groupname>
      <gid>1000</gid>
    </group>
  </groups>
</profile>
